<?xml version="1.0" encoding="UTF-8"?>
<assembly>
    <id>proj</id>
    <formats>
        <format>zip</format>
    </formats>

    <fileSets>
        <fileSet>
            <directory>.</directory>
            <outputDirectory></outputDirectory>
            <useDefaultExcludes>false</useDefaultExcludes>
            <includes>
                <include>src/**/*</include>
                <include>*</include>
                <include>target/*.war</include>
            </includes>
            <excludes>
                <exclude>.*</exclude>
                <exclude>*.zip</exclude>
            </excludes>
        </fileSet>
    </fileSets>

