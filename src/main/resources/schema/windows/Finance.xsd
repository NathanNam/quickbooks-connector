					<xs:element name="CCDetail" type="CreditCardPayment" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: [i]Unsupported field.[/i]
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ProcessPayment" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Indicates that the payment should be processed by merchant account service. Not Supported By Quickbooks Windows Desktop. Valid for QBO companies only with credit card processing.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AppliedTo" type="RefundAppliedTo" minOccurs="0"
						maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Specifies a credit transaction to be refunded by this credit card refund. At least one credit transaction of the supported types must be provided: credit memo, receive payment, or general journal entry. [br/] Required for the Add request.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CreditChargeInfo">
		<xs:annotation>
			<xs:documentation>
        Product: QBW
        Description: Hold credit-card information to request a credit card payment from a merchant account service, but NOT any response or authorization information from the merchant account service provider -- see CreditChargeResponse
      </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Number" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: Credit Card account number, as printed on the card.  Should not have whitespace or formatting characters.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Token" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: Tokenized representation of the credit card account number
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Type" type="CreditCardTypeEnum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: Visa, MasterCard, American Express, etc...
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NameOnAcct" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: Account holder name, as printed on the card.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CcExpirMn" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: Expiration Month on card, expressed as a nu 1 = January, 2 = February, etc...
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CcExpirYr" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: Expiration Year on card, expressed as a 4 digit number 1999, 2003, etc...
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BillAddrStreet" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: Credit card holder billing address of record -- the street address to which credit card statements are sent.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ZipCode" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: Credit card holder billing Postal Code. 5 digits in the USA
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Cvv" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: Credit card validation code (sometimes printed on back of card)
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CommercialCardCode" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: Code associated with commercial cards - purchase, corporate, and business cards. Lower transaction fee rates apply when these cards are used and this field is provided.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CCTxnMode" type="CCTxnModeEnum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: specifies a credit card transaction mode used in Credit Card payment transactions. May have one of the following values: CardNotPresent [DEFAULT], CardPresent
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CCTxnType" type="CCTxnTypeEnum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: Specifies the type of credit card transaction. May have one of the following values: Authorization, Capture, Charge, Refund, VoiceAuthorization
          </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CreditChargeResponse">
		<xs:annotation>
			<xs:documentation>
        Product: QBW
        Description: Holds credit-card transaction response information from a merchant account service, but not any credit card or payment request information - see CreditChargeInfo
      </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CCTransId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: Unique identifier of the payment transaction. It can be used to track the status of transactions, or to search transactions.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Status" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: Indicates the status of the payment transaction. Possible values include Completed, Prepared, Sent, Unknown, Voided. The values are defined in the CCPaymentStatusEnum.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ResultCode" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: Numeric code specifying the result of the transaction.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ResultMsg" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: Text specifying the result of the transaction.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MerchantAcctNum" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: Merchant account number associated with the credit card transaction.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CardSecurityCodeMatch" type="CCSecurityCodeMatchEnum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: Result of comparing the security code supplied in the credit card transaction request with the code on file with the card issuer. Possible values are Pass, Fail, and NotAvailable.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AuthCode" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: Code returned from the credit card processor to indicate that the charge will be paid by the card issuer.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AvsStreet" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: The AVS (Address Verification Service) result for the street address supplied in the transaction.  Possible values are Pass, if the information matches the information on file with the cardholder's account, Fail, or NotAvailable. The values are defined in the CCAVSMatchEnum.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AvsZip" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: The AVS (Address Verification Service) result for the zip code supplied in the transaction.  Possible values are Pass, if the information matches the information on file with the cardholder's account, Fail, or NotAvailable. The values are defined in the CCAVSMatchEnum.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SecurityCode" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: CardCode or Card Id field that can be optionally provided to use additional security features of credit card authorization. It is typically a 3 digit number located on the back of most credit cards. For American Express, it is a 4 digit number on the front. Unsupported field.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ReconBatchId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: Indicates which deposit batch the transaction belongs to. Allows for integration with Intuit MAS Service and QBFS: enables reconciliation between what’s charged on credit card and what is already deposited into bank.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentGroupingCode" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: Code that indicates membership in a group of card types that are normally deposited together.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TxnAuthorizationTime" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: Timestamp indicating the time in which the card processor authorized the transaction.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TxnAuthorizationStamp" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: This value is used to support the credit card transaction reconciliation.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ClientTransID" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: An identifier returned in settlement data used to support the credit card transaction reconciliation.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CreditMemo">
		<xs:annotation>
			<xs:documentation>
        Product: QBW
        Description: Financial transaction representing a refund (or credit) of payment or part of a payment for goods or services that have been sold.
        EndPoint: services.intuit.com
        Business Rules: None        
      </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CdmBase">
				<xs:sequence>
					<xs:element name="Header" type="CreditMemoHeader" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Provides information that pertains to the entire CreditMemo
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Line" type="CreditMemoLine" minOccurs="0"
						maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Provides the line level detail of the services offered or items ordered and the associated detail.  It can be aligned with the Sales Order lines.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TaxLine" type="TaxLine" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Provides the line level detail on a specific tax collected on all taxable lines on the entire transaction.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CreditMemoHeader">
		<xs:annotation>
			<xs:documentation>
        Product: QBW
        Description: Information that pertains to the entire CreditMemo
      </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="HeaderSales">
				<xs:sequence>
					<xs:group ref="ARAccountReferenceGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: ARAccountReferenceGroup Identifies the AR Account to be used for this Credit Memo. [br/] The AR Account should always be specified or a default will be used.
              </xs:documentation>
						</xs:annotation>
					</xs:group>
					<xs:group ref="SalesTermReferenceGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Identifies the payment term for an Order or Invoice. Supports the ability to specify an absolute due date, a number of days from a start date, a percent discount or an absolute discount. A common term like "Net30Days" is explicitly code as either a calculated DueDateTime or a PaymentTermsDateTime and NumberOfDays.
              </xs:documentation>
						</xs:annotation>
					</xs:group>
					<xs:element name="DueDate" type="IDSDate" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Date when the Invoice is to be paid.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BillAddr" type="PhysicalAddress" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Identifies the physical address where the bill or invoice is to be sent.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ShipAddr" type="PhysicalAddress" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Identifies the address where the goods must be shipped. In a lot of cases, this is the same as the Billing Address. In multi-organization purchasing, the Shipping address may be different from the Billing address.[br/]If ShipAddr is not specified, and a default ship-to address is specified in QuickBooks for this customer, the default ship-to address will be used by QuickBooks.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BillEmail" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Identifies the email address where the bill or invoice is to be sent. [br/] [i]Unsupported field.[/i]
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:group ref="ShipMethodReferenceGroup" minOccurs="0"/>
					<xs:element name="RemainingCredit" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Indicates the total credit amount still available to apply towards the payment.[br/] Non QB-writable.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Detail" type="PaymentDetail" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Detailed information about the payment for this sale.  It can be cash, check, or credit card.[br/] [i]Unsupported field.[/i]
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:group ref="QboDiscountGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBO
                Description: Specifies the discount-related data: amount or rate, account.
QBO only fields.
							</xs:documentation>
						</xs:annotation>
					</xs:group>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CreditMemoLine">
		<xs:annotation>
			<xs:documentation>
        Product: QBW
        Description: A service or product sold, along with quantity, price, and other descriptive information
      </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="LineSales">
				<xs:sequence>
					<xs:element name="ServiceDate" type="IDSDate" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Indicates the date when the service that is billable to a customer is going to be performed
              </xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CurrencyInfo">
		<xs:annotation>
			<xs:documentation>
				Product: QBW, QBO
				Description: The currency object contains all of the information needed to display and use a foreign currency. For built-in currencies, the name and currency code values are internationally accepted values and thus are not editable. The comma format is editable, as is the IsActive status. For user-defined currencies, every value in the object is editable including name and currency code.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CdmBase">
				<xs:sequence>
					<xs:element name="Name" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								Product: QBW, QBO
								Description: The name of the currency [br/] Cloud Max Length: 64 [br/]
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Active" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								Product: QBW, QBO
								Description: Whether or not active inactive currency may be hidden from most display purposes and may not be used on financial transactions.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CurrencyCode" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								Product: QBW, QBO
								Description:  The three-letter currency code.  For standard currencies this should be one of the ISO 4217 Currency codes defined in the currencyCode enum.  For custom currencies (i.e. a Linden Dollar) this may be any three letter code. 
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="UserDefined" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								Product: QBW, QBO
								Description: false if this is a standard built-in currency, true if this is a user-defined currency
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CurrencyFormat" type="CurrencyFormatInfo" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								Product: QBW, QBO
								Description: Information regarding the proper string formatting of the currency
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ExchangeRate" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								Product: QBW, QBO
								Description: The exchange rate currently in effect as of the date specified in AsOfDate expressed as a multiplier of the currency value to get the home currency value.  Values greater than 1 indicate a currency with more buying power than the home currency, while values less than one indicate a currency with less buying power.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AsOfDate" type="IDSDate" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								Product: QBW, QBO
								Description: The date as of which the current exchange rate applies.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="CurrencyFormatInfo">
		<xs:sequence>
			<xs:element name="DecimalPlaces" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Product: QBW, QBO
						Description: The number of places to the right of the decimal point.  Default is 2.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DecimalSeparator" type="separatorEnum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Product: QBW, QBO
						Description: The character used as a decimal point in this currency.  Dollars would use a period, European currencies may use a comma.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ValueGroupSeparator" type="separatorEnum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Product: QBW, QBO
						Description: The character used to separate groups of digits in the whole-number value of the currency.  For example, dollars are grouped in thousands by the comma (1,000,000)
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ValueGrouping" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Product: QBW, QBO
						Description: The standard grouping for this currency.  For example, some currencies group by thousands and then tens: XX_XX_XXX
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CustomerMsg">
		<xs:annotation>
			<xs:documentation>
        Product: QBW
        Description: A standard message to a customer that can be included at the bottom of a sales form. A customer message or a customer message list appears at the bottom of a sales form. For example, you can create a thank you message using the CustomerMsg, and include this message in any invoice.
        EndPoint: services.intuit.com
        Business Rules: None        
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CdmBase">
				<xs:sequence>
					<xs:element name="Name" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Contains the message to a customer.[br/] Cloud Max Length: 256 [br/] Required for the Add request. [br/] Max Length: 101
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Active" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Whether or not active inactive customer message may be hidden from most display purposes and may not be used on financial transactions.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CustomerRef">
		<xs:group ref="CustomerReferenceGroup"/>
	</xs:complexType>
	<xs:complexType name="Deposit">
		<xs:annotation>
			<xs:documentation>
        Product: QBW
        Description: Transaction recording the transfer of money from the Undeposited Funds account to the Bank account.
      </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CdmBase">
				<xs:sequence>
					<xs:element name="Header" type="DepositHeader" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Provides information that pertains to the entire Deposit.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Line" type="DepositLine" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Provides the line level detail of the Deposit transaction.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DepositHeader">
		<xs:annotation>
			<xs:documentation>
		        Product: QBW
		        Description: Information that pertains to the entire Deposit.
	      	</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="HeaderBase">
				<xs:sequence>
					<xs:group ref="DepositToAccountReferenceGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
				                Product: QBW
				                Description: DepositToAccountReferenceGroup Identifies the Asset Account (bank account) to be used for this Deposit. [br/] Required for the Add request. [br/]
							</xs:documentation>
						</xs:annotation>
					</xs:group>
					<xs:element name="CashBack" type="CashBackInfo" minOccurs="0"/>
					<xs:element name="TotalAmt" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
				                Product: QBW
								Query-able: QBW
				                Description: Total amount of Deposit. [br/] Read only.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DepositLine">
		<xs:annotation>
			<xs:documentation>
        Product: QBW
        Description: Each deposit line describes one deposit into the account indicated by the Deposit To Account data in the header. Deposit lines are added as either received payments (described by a TxnID and possibly a TxnLineID) or as transfers from one account into another (where at least the Account data must be specified).
      </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="LineBase">
				<xs:sequence>
					<xs:group ref="TxnLineReferenceGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: The reference to the payment transaction which is being deposited.
              </xs:documentation>
						</xs:annotation>
					</xs:group>
					<xs:element name="TxnType" type="TxnTypeEnum" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: For information purpose only - the type of the payment transaction.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:group ref="AccountReferenceGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Specifies the Account that is the source of the amount to be deposited.[br/] Required for the Add request if there is no reference to the payment transaction specified.
							</xs:documentation>
						</xs:annotation>
					</xs:group>
					<xs:group ref="EntityReferenceGroup" minOccurs="0"/>
					<xs:group ref="PaymentMethodReferenceGroup" minOccurs="0"/>
					<xs:group ref="ClassReferenceGroup" minOccurs="0"/>
					<xs:element name="Amount" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
				                Product: QBW
				                Description: Amount of the deposit.[br/] Required for the Add request if the Deposit is specified as a transfer rather than the payment transaction reference.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CheckNum" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
							Product: QBW
							Description: Check number.
						</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Discount">
		<xs:annotation>
			<xs:documentation>
        Product: QBW
        Description: Identifies the discount or allowance that the Customer is to receive off of the TotalAmount owed.
        EndPoint: services.intuit.com
        Business Rules: None        
      </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CdmBase">
				<xs:sequence>
					<xs:element name="Name" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: User recognizable name for the Discount.[br/] Cloud Max Length: 4000 [br/] Required for the Add request. [br/] Max Length: 31
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Desc" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: User entered description for the Discount which may include user entered details with reason for the reduction of the amount.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Taxable" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: If true, calculate the sales tax first, and then apply the discount. If false, subtract the discount first and then calculate the sales tax.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Active" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Whether or not active inactive discounts may be hidden from most display purposes and may not be used on financial transactions.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:choice minOccurs="0">
						<xs:element name="Percent" type="xs:decimal" minOccurs="0">
							<xs:annotation>
								<xs:documentation>
                  Product: QBW
                  Description: Percentage by which the amount due is reduced, from 0% to 100%. To enter a discount of 8.5% use 8.5, not 0.085.
                </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Amount" type="Money" minOccurs="0">
							<xs:annotation>
								<xs:documentation>
                  Product: QBW
                  Description: Amount by which the amount due is reduced.
                </xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
					<xs:group ref="DiscountAccountReferenceGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Indicates the account used to track discounts given to customers on sales. When an income account is used to track discounts on sales, it is usually called a "contra-income" account. [br/] Required for the Add request.
							</xs:documentation>
						</xs:annotation>
					</xs:group>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EmployeeRef">
		<xs:group ref="EmployeeReferenceGroup"/>
	</xs:complexType>
	<xs:complexType name="Estimate">
		<xs:annotation>
			<xs:documentation>
        Product: QBO
        Description: The Estimate represents a proposal for a financial transaction from a business to a customer for goods or services proposed to be sold, including proposed pricing. It is also known as quote. Note: Available only to the QuickBooks Online Plus subscribers.
        EndPoint: qbo.intuit.com
        Business Rules: [li] An estimate must have at least one line that describes an item. [/li][li] An estimate must have a reference to a customer in the header. [/li][li] If you submit a query with the filter IncludeDiscountLineDetails, the system retrieves either DiscountAmount or DiscountRatePercent with associated values[/li]        
        Product: QBW
        Description: An estimate is a description of a sale that you propose to a current or prospective customer. An estimate contains a list of tasks that you can perform for, or products that you can provide to customers. An estimate is also called a bid or proposal. An estimate is a non-posting transaction that does not affect the balance sheet or income statement of a company.
        EndPoint: services.intuit.com
        Business Rules: [li] An estimate must have at least one line that describes an item.[/li] [li] An estimate must have a reference to a customer in the header.[/li] [li] If you submit a query with the filter IncludeDiscountLineDetails, the system retrieves either DiscountAmount or DiscountRatePercent with associated values[/li]       
      </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CdmBase">
				<xs:sequence>
					<xs:element name="Header" type="EstimateHeader" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: ALL
                Description: Information that pertains to the entire Estimate
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Line" type="EstimateLine" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>
                Product: ALL
                Description: A service or product proposed to be sold along with quantity, price and other descriptive information.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TaxLine" type="TaxLine" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Provides the line level detail on a specific tax collected on all taxable lines on the entire transaction.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EstimateHeader">
		<xs:annotation>
			<xs:documentation>
        Product: ALL
        Description: Information that pertains to the entire Estimate
      </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="HeaderSales">
				<xs:sequence>
					<xs:group ref="SalesTermReferenceGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: ALL
                Description: Identifies the payment term for an Order or Invoice. Supports the ability to specify an absolute due date, a number of days from a start date, a percent discount or an absolute discount. A common term like "Net30Days" is explicitly code as either a calculated DueDateTime or a PaymentTermsDateTime and NumberOfDays.
              </xs:documentation>
						</xs:annotation>
					</xs:group>
					<xs:element name="DueDate" type="IDSDate" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
							Product: QBW
							Description: Date when the Invoice is to be paid.[br /]
[b][i]QuickBooks Note[/i][/b] [br /]
If DueDate is not included when creating an invoice, QuickBooks may determine the due date according to the terms set for this customer. [br /]
If the Terms are not provided, the Due Date is set to the transaction date.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BillAddr" type="PhysicalAddress" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
							Product: QBW
							Description: Identifies the physical address where the bill or invoice is to be sent.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ShipAddr" type="PhysicalAddress" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
							Product: QBW
							Description: Identifies the address where the goods must be shipped. In a lot of cases, this is the same as the Billing Address. In multi-organization purchasing, the Shipping address may be different from the Billing address.[br /]
[b][i]QuickBooks Note[/i][/b] [br /]
If ShipAddr is not specified, and a default ship-to address is specified in QuickBooks for this customer, the default ship-to address will be used by QuickBooks.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BillEmail" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: ALL
                Description: Identifies the email address where the bill or invoice is to be sent.
                [b]QuickBooks Notes[/b][br /]
[i]Unsupported field.[/i]
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:group ref="QboDiscountGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
							Product: QBO
							Description: Specifies the discount-related data: amount or rate, account.
QBO only fields.
							</xs:documentation>
						</xs:annotation>
					</xs:group>
					<xs:element name="ExpirationDate" type="IDSDate" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
							Product: QBO
							Description: Date by which estimate must be accepted before invalidation.
QBO only field.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AcceptedBy" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
							Product: QBO
							Description: Name of customer who accepted the estimate.
QBO only field.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AcceptedDate" type="IDSDate" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
							Product: QBO
							Description: Date estimate was accepted.
QBO only field.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EstimateLine">
		<xs:annotation>
			<xs:documentation>
        Product: ALL
        Description: A service or product proposed to be sold along with quantity, price and other descriptive information.
      </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="LineSales"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="FixedAsset">
		<xs:annotation>
			<xs:documentation>
        Product: QBW
        Description: Fixed assets represent the business assets that you do not convert to cash within one year of operation. A fixed asset is usually integral to your business operations. For example, a truck, cash register, or computer.
        EndPoint: services.intuit.com
        Business Rules: [li]If ItemID or ItemType are specified for a transaction, do not specify UnitPrice for the fixed asset.[/li]        
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CdmBase">
				<xs:sequence>
					<xs:element name="Name" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
				Query-able: QBW
                Description: User recognizable name for the Fixed Asset Item.[br/]
                Cloud Max Length: 4000 [br/] Required for the Add request. [br/] Max Length: 31
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Active" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Whether or not active inactive fixed assets may be hidden from most display purposes and may not be used on financial transactions.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AcquiredAs" type="AcquiredAsEnum" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Specifies whether the asset is new or used. This will aid in calculating depreciation.[br/] Required for the Add request. [br/]
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PurchaseDesc" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: User entered purchase description for the fixed asset which may include user entered information to further describe the details of the purchase.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PurchaseDate" type="IDSDate" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Specifies the date the asset was purchased or acquired.[br/] Required for the Add request. [br/]
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PurchaseCost" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
				Query-able: QBW
                Description: Specifies the asset's purchase price.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Vendor" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Specifies the name of the vendor or payee from whom the asset was purchased.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AssetAccountRef" type="AccountRef" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Indicates the Fixed Asset account that tracks the current value of the asset. If the same account is used for all fixed assets, the current balance of this account will represent the current total value of the fixed assets.[br/] Required for the Add request. [br/]
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SalesDesc" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: User entered sales description for the fixed asset which may include user entered information to further describe the details of the sales.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SalesDate" type="IDSDate" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Specifies the date the asset was sold.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SalesPrice" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Specifies the amount for which the asset was sold.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SalesExpense" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Specifies any additional expenses incurred during the sale of the asset.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Location" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Information about where the asset is located or has been placed into service.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PONumber" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: The purchase order number if a purchase order was used to buy the asset.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SerialNumber" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: The serial number of the asset. For a vehicle, it can be the VIN.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="WarrantyExpDate" type="IDSDate" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: The date the warranty for the asset expires.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Desc" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Any description of the asset, like maker, brand, and so on.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Notes" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Notes about the asset that might help to track it properly, such as notes about repairs or upkeep.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AssetNum" type="xs:integer" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: QBWT only: asset number. Maintained by the QB Fixed Asset Manager.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CostBasis" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: QBWT only: The total cost of the fixed asset. This can include the cost of improvements or repairs. This amount is used to calculate depreciation. Maintained by the QB Fixed Asset Manager.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Depreciation" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: QBWT only: the total amount of depreciation expense since the fixed asset was acquired as of the end of the year. Maintained by the QB Fixed Asset Manager.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BookValue" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: QBWT only: the asset's cost or basis less accumulated depreciation as of the end of the year. Maintained by the QB Fixed Asset Manager.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="HeaderBase">
		<xs:annotation>
			<xs:documentation>
        Product: ALL
        Description: Subset of data that is common to all transaction types.
      </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DocNumber" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBO
			Query-able: QBO
            Description: The primary document number, humanly identifiable, for this transaction.
            Product: QBW
            Description: The primary document number, humanly identifiable, for this transaction.[br/] Cloud Max Length: 1024 [br/] If it is not provided, QuickBooks business logic will assign the document number using the "next in sequence" algorithm.[br/] Max Length: 11; for Payment, Bill,ItemReceipt and VendorCredit, Max Length: 20
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TxnDate" type="IDSDate" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: ALL
			Query-able: ALL
            Description: The nominal, user entered, date of the transaction.  Often, but not required to be, the date the transaction was created in the system. For "posting" transactions, this is the posting date that affects financial statements.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Currency" type="currencyCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: Currency code that is common to all amounts on the associated transaction.[br/] Non QB-writable.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Msg" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBO
            Description: User entered message to the customer.
            Product: QBW
            Description: User entered message to the customer.[br/] Non QB-writable.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Note" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: User entered organization-private note about the transaction.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Status" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
            Product: QBW
            Description: The status of the associated transaction (the transaction types are described in the TxnTypeEnum).  Depending on the transaction type the status may have different values. For example, Sales Transactions acceptable values are defined in PaymentStatusEnum. For Estimate, the values accepted are defined in EstimateStatusEnum, and for BuildAssembly the values are defined in the BuildAssemblyStatusEnum.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="HeaderPurchase">
		<xs:annotation>
			<xs:documentation>
        Product: ALL
        Description: Financial Transaction information that pertains to the entire Bill.
      </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="HeaderBase">
				<xs:sequence>
					<xs:group ref="VendorReferenceGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBO
                Description: Identifies the sales tax agency (vendor) to whom the tax is remitted.
                Product: QBW
                Description: The person or organization that the AP transaction was received from, or the Purchase Order (PO) is sent to.[br/] Valid Vendor Name or Id is required for the Add request for all AP transactions.[br/] Required for the Add request.
							</xs:documentation>
						</xs:annotation>
					</xs:group>
					<xs:element name="BillEmail" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: The email address to which this bill is/was sent. [br/] Non QB-writable.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ReplyEmail" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: The email address to which inquiries about the bill may be directed. (Also appropriate for paypal payments). [br/] Non QB-writable.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TotalAmt" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBO
                Description: The total amount due, determined by taking the sum of all lines associated.  This includes all charges, allowances, taxes, discounts, etc... In case of the "credit" transaction (e.g. Vendor Credit) represents the amount of the received credit.
                Product: QBW
				Query-able: QBW
                Description: The total amount due, determined by taking the sum of all lines associated.  This includes all charges, allowances, taxes, discounts, etc... In case of the "credit" transaction (e.g. Vendor Credit) represents the amount of the received credit. [br/] Non QB-writable.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="HeaderSales">
		<xs:annotation>
			<xs:documentation>
        Product: ALL
        Description: Super-set of data that is associated with the Sale transaction types.
      </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="HeaderBase">
				<xs:sequence>
					<xs:group ref="CustomerReferenceGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBO
                Description: The person or organization that is the end customer of the goods or services being provided in the associated transaction. For example, if the transaction is associated with the Job or Project, like "John Doe:Kitchen", this object must reference "John Doe".
                Product: QBW
                Description: The person or organization that is the end customer of the goods or services being provided in the associated transaction. For example, if the transaction is associated with the Job or Project, like "John Doe:Kitchen", this object must reference "John Doe".[br/]Valid Customer Name or Id is required for the Add request for all AR transactions with the exception of Sales Receipt.[br/] Required for the Add request.
							</xs:documentation>
						</xs:annotation>
					</xs:group>
					<xs:group ref="JobReferenceGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Identifies the actual Job or Project the goods or services are provided to.[br/] [b][i]QuickBooks Note[/i][/b] [br/] For example, if the transaction is associated with the Job or Project, like "John Doe:Kitchen", this object must reference "John Doe:Kitchen", while the CustomerName must contain "John Doe".
							</xs:documentation>
						</xs:annotation>
					</xs:group>
					<xs:group ref="RemitToReferenceGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Identifies the party or location that the payment is to be remitted to or sent to.[br/] Non QB-writable.
							</xs:documentation>
						</xs:annotation>
					</xs:group>
					<xs:group ref="ClassReferenceGroup" minOccurs="0"/>
					<xs:group ref="SalesRepReferenceGroup" minOccurs="0"/>
					<xs:group ref="SalesTaxCodeReferenceGroup" minOccurs="0"/>
					<xs:element name="PONumber" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Cloud Max Length: 256 [b][i]QuickBooks Note[/i][/b] [br/] Max Length: 13
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="FOB" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: "Free On Board", specifies the terms between buyer and seller regarding transportation costs; does not have any bookkeeping implications. Cloud Max Length: 256 [b][i]QuickBooks Note[/i][/b] [br/] Max Length: 13
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ShipDate" type="IDSDate" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Agreed upon date for delivery of goods or services
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SubTotalAmt" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: Indicates the sub total amount of the object associated. This includes the total of all the charges, and allowances but excludes the amount of tax. [br/] Non QB-writable.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:group ref="TaxChoiceGroup" minOccurs="0"/>
					<xs:element name="TaxRate" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: ALL
                Description: The total tax rate used to calculate the tax amount on the transaction. To enter a tax rate of 8.5%, enter 8.5, not 0.085. [br/] Non QB-writable.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TaxAmt" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBO
                Description: Indicates the total tax amount on the associated transaction. The subtotal plus the tax amount equals the total amount. It is a calculated field by QuickBooks business logic.
                Product: QBW
                Description: Indicates the total tax amount on the associated transaction. The subtotal plus the tax amount equals the total amount. It is a calculated field by QuickBooks business logic.Non QB-writable.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TotalAmt" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
				Query-able: QBW
                Description: Indicates the total amount of the object associated. This includes the total of all the charges, allowances and taxes. It is a calculated field by QuickBooks business logic. [br/] Non QB-writable.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ToBePrinted" type="xs:boolean" minOccurs="0"/>
					<xs:element name="ToBeEmailed" type="xs:boolean" minOccurs="0"/>
					<xs:element name="Custom" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                Product: QBW
                Description: free form text[br/] Cloud Max Length: 4000 [/b] [br/] Max Length: 29
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
